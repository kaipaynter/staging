{
  "name": "staging",
  "description": "Kai Paynter's staging website",
  "version": "1.0.0",
  "author": "Boipelo Mawasha",
  "homepage": "https://staging.kaipaynter.com",
  "dependencies": {
    "agentkeepalive": "^4.1.4",
    "babel-plugin-remove-graphql-queries": "^3.9.0",
    "bootstrap": "^5.0.2",
    "gatsby": "^3.9.1",
    "gatsby-background-image": "^1.5.3",
    "gatsby-image-background-slider": "0.0.4",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-offline": "^4.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-sass": "^4.9.0",
    "gatsby-plugin-segment-js": "^3.6.1",
    "gatsby-plugin-sitemap": "^4.5.0",
    "gatsby-plugin-typescript": "^3.9.0",
    "gatsby-source-filesystem": "^3.9.0",
    "node-sass": "^6.0.1",
    "react": "^16.13.0",
    "react-bootstrap": "^1.6.1",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-scroll-to-element": "^0.2.0",
    "react-scrollspy": "^3.4.2",
    "react-slick": "^0.28.1",
    "react-waypoint": "^9.0.2",
    "slick-carousel": "^1.8.1",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -d gh-pages",
    "branchSetup": "git checkout -b gh-pages",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "reset": "git checkout master",
    "deploy:full": "npm run branchCleanup && npm run branchSetup && npm run deploy && npm run reset"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaipaynter/staging"
  }
}
