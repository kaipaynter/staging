{
  "name": "staging",
  "version": "0.0.1",
  "description": "Kai Paynter's staging website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaipaynter/staging.git"
  },
  "author": {
    "name": "Boipelo Mawasha",
    "email": "beepa@kaipaynter.com"
  },
  "dependencies": {
    "gatsby": "^3.12.0",
    "gatsby-plugin-manifest": "^3.12.0",
    "gatsby-plugin-offline": "^4.12.0",
    "gatsby-plugin-react-helmet": "^4.12.0",
    "gatsby-plugin-sass": "^4.12.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-images": "^1.2.0-beta.7",
    "react-scrollspy": "^3.4.2",
    "simple-react-lightbox": "^3.6.9-0",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -d gh-pages",
    "branchSetup": "git checkout -b gh-pages",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "reset": "git checkout new-site",
    "deploy:full": "npm run branchCleanup && npm run branchSetup && npm run deploy && npm run reset"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "gatsby",
    "gatsby-starter",
    "gatsby-starter-phantom"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaipaynter/staging/issues"
  },
  "homepage": "https://staging.kaipaynter.com"
}
