{
  "name": "staging",
  "description": "Kai Paynter's staging website",
  "version": "1.0.0",
  "author": "Boipelo Mawasha",
  "homepage": "https://kaipaynter.github.io/staging",
  "dependencies": {
    "gatsby": "^2.19.32",
    "gatsby-plugin-manifest": "^2.2.44",
    "gatsby-plugin-offline": "^3.0.37",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sass": "^2.1.30",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-scroll-to-element": "^0.2.0",
    "react-scrollspy": "^3.4.2",
    "react-waypoint": "^9.0.2",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -d gh-pages && git push origin --delete gh-pages",
    "branchSetup": "git checkout -b gh-pages && git push --set-upstream origin gh-pages",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy:full": "npm run branchCleanup && npm run branchSetup and npm run deploy && npm run reset",
    "reset": "git checkout master"
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaipaynter/staging"
  }
}
